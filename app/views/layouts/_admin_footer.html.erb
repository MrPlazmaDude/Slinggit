<footer class="adminFooter">
  <div class="wrapper">
    <div id="loggedInAs">
      <p>Logged in as: <%= current_user.name %></p>
      <table border="1">
        <tr>
          <td>id</td>
          <td><%= current_user.id %></td>
        </tr>
        <tr>
          <td>email</td>
          <td><%= current_user.email %></td>
        </tr>
        <tr>
          <td>account status</td>
          <td><%= current_user.status %></td>
        </tr>
        <tr>
          <td>created on</td>
          <td><%= current_user.created_at %></td>
        </tr>
      </table>
    </div>
  </div>
</footer>